{
  "components": {
    "schemas": {
      "Todo": {
        "properties": {
          "done": {
            "type": "boolean"
          },
          "title": {
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "info": {
    "description": "-",
    "title": "Sample API",
    "version": "0.0.0"
  },
  "openapi": "3.0.0",
  "paths": {
    "/AddTodo": {
      "post": {
        "description": "Adds a new todo to your todo list",
        "operationId": "m/design.AddTodo",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "todo": {
                    "$ref": "#/components/schemas/Todo"
                  }
                },
                "required": [
                  "todo"
                ],
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Todo"
                }
              }
            },
            "description": ""
          },
          "default": {
            "description": ""
          }
        }
      }
    },
    "/ListTodo": {
      "post": {
        "description": "Returns the current todo list",
        "operationId": "m/design.ListTodo",
        "parameters": [
          {
            "in": "query",
            "name": "all",
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/Todo"
                  },
                  "type": "array"
                }
              }
            },
            "description": ""
          },
          "default": {
            "description": ""
          }
        }
      }
    }
  },
  "servers": [
    {
      "url": "http://localhost:8888",
      "description": "local development server"
    }
  ]
}
